<body class="hold-transition skin-blue sidebar-mini">
    <div class="wrapper">
        <header class="main-header">
            <!-- Pic -->
            <a href="@Url.Action("Dashboardv1", "Dashboard")" class="logo">
                <!-- Modified to be the home controller (FR)-->
                <!-- mini logo for sidebar mini 50x50 pixels -->
                <span class="logo-mini"><b>A</b>LT</span>
                <!-- logo for regular state and mobile devices -->
                <span class="logo-lg"><b>OnlineCourse</b></span>
            </a>
            <!-- Header Navbar: style can be found in header.less -->
            <nav class="navbar navbar-static-top">
                <!-- Sidebar toggle button-->
                <a href="#" class="sidebar-toggle" data-toggle="offcanvas" role="button">
                    <span class="sr-only">Toggle navigation</span>
                </a>
                <div class="navbar-custom-menu">
                    <ul class="nav navbar-nav">
                        <!-- Messages: style can be found in dropdown.less-->
                        <!-- User Account: style can be found in dropdown.less -->
                        <li class="dropdown user user-menu">

                            <a asp-action="LogOut" asp-controller="Account" asp-area=""><i class="fa fa-sign-out "></i> خروج</a>


                        </li>
                        <!-- Control Sidebar Toggle Button -->

                    </ul>
                </div>
            </nav>
        </header>




        <!-- Left side column. contains the logo and sidebar -->
        <aside class="main-sidebar">
            <!-- sidebar: style can be found in sidebar.less -->
            <section class="sidebar">
                <!-- Sidebar user panel -->
                <div class="user-panel">
                    <div class="pull-left image">

                        @{
                            var path = await _user.GetUserProfilePicPath();
                        }
                        <img src="/uploads/galleries/@(string.IsNullOrEmpty(path)?"Admin.png":path)" class="img-circle" alt="">

                    </div>
                    <div class="pull-right info" style="position:unset;">
                        <p>@(await _user.GetFullName())</p>
                        <a href="#"><i class="fa fa-circle text-success"></i> Online</a>
                    </div>
                </div>
                <!-- search form -->
                <form action="#" method="get" class="sidebar-form">
                    <div class="input-group">
                        <input type="text" name="q" class="form-control" placeholder="جستجو">@*@localizer["Search"]*@
                        <span class="input-group-btn">
                            <button type="submit" name="search" id="search-btn" class="btn btn-flat">
                                <i class="fa fa-search"></i>
                            </button>
                        </span>
                    </div>
                </form>
                <!-- /.search form -->
                <!-- sidebar menu: : style can be found in sidebar.less -->
                <ul class="sidebar-menu">
                    <li class="header" style="color: white"> @*@localizer["MainMenu"]*@ منوی اصلی</li>
                    <!-- Add new comment here (FR) -->
                    <li class="treeview">
                        <a asp-action="Index" asp-controller="CheckOut" asp-area="Student">
                            <i class="fa fa-credit-card"></i> <span>مشاهده صورت حساب</span>
                        </a>
                    </li>
                    <li class="treeview">
                        <a asp-action="Index" asp-controller="Enrollments" asp-area="Student">
                            <i class="fa fa-database"></i> <span>دروس اخذ شده</span>
                        </a>
                    </li>
                    <li class="treeview">
                        <a asp-action="Index" asp-controller="Invoices" asp-area="Student">
                            <i class="fa fa-bank"></i> <span>سوابق مالی و پرداخت ها</span>
                        </a>
                    </li>
                    <li class="treeview">
                        <a asp-action="Index" asp-controller="Profile" asp-area="Student">
                            <i class="fa fa-user"></i> <span>پروفایل کاربری</span>
                        </a>
                    </li>
                </ul>
            </section>
            <!-- /.sidebar -->
        </aside>










